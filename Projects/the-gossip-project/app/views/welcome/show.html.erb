<h1>Welcome <%= @first_name %>!</h1>
<div class="container">
    <% @list_gossips.each_with_index do |gossip|%>
        <p>
            <%= gossip.title %>
            <!-- <%= gossip.id %> -->
            <em><%= gossip.user.first_name%>
            <%= gossip.user.last_name%></em>
        </p>
    <% end %>
</div>
